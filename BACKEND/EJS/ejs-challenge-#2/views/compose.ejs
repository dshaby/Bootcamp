<%- include("partials/header"); %>
    <h3>Compose!</h3>
    <form action="/" method="post">
        <div class="mb-3">
            <label for="formTitle" class="form-label">Title</label>
            <input type="text" class="form-control form-control-lg" name="formTitle">
        </div>
        <div class="mb-3">
            <label for="formBody" class="form-label">Body</label>
            <textarea class="form-control" name="formBody" rows="5"></textarea>
        </div>
        <div class="d-grid gap-2 d-sm-block">
            <button type="submit" class="btn btn-primary">Submit</button>
        </div>
    </form>
    <%- include("partials/footer"); %>